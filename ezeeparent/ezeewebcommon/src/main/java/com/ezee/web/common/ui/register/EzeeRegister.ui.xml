<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="com.ezee.web.common.ui.css.EzeeDefaultResources" field="res" />

	<g:HTMLPanel>
		<g:HorizontalPanel height="100%" width="100%" verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER"
			styleName="{res.css.header}">
			<g:AbsolutePanel height="800px" width="1000px">
				<g:at top="203" left="319">
					<g:HTML wordWrap="false" width="376px" height="52px" ui:field="lblTitle" styleName="{res.css.headerLabel}" />
				</g:at>
				<g:at top="260" left="316">
					<g:Label text="Firstname" styleName="{res.css.gwtLabelMedium}" />
				</g:at>
				<g:at top="294" left="316">
					<g:Label text="Lastname" styleName="{res.css.gwtLabelMedium}" />
				</g:at>
				<g:at top="329" left="316">
					<g:Label text="Username" styleName="{res.css.gwtLabelMedium}" />
				</g:at>
				<g:at top="363" left="316">
					<g:Label text="Password" styleName="{res.css.gwtLabelMedium}" />
				</g:at>
				<g:at top="397" left="316">
					<g:Label text="eMail" styleName="{res.css.gwtLabelMedium}" />
				</g:at>
				<g:at top="260" left="482">
					<g:TextBox ui:field="txtFirstname" styleName="{res.css.gwtTextBoxMedium}" />
				</g:at>
				<g:at top="294" left="482">
					<g:TextBox ui:field="txtLastname" styleName="{res.css.gwtTextBoxMedium}" />
				</g:at>
				<g:at top="329" left="482">
					<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtUsername" />
				</g:at>
				<g:at top="363" left="482">
					<g:PasswordTextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtPassword" />
				</g:at>
				<g:at top="397" left="482">
					<g:TextBox ui:field="txtEmail" styleName="{res.css.gwtTextBoxMediumEmail}" />
				</g:at>
				<g:at top="431" left="507">
					<g:Button text="Back" ui:field="btnBack" styleName="{res.css.gwtButton}" />
				</g:at>
				<g:at top="431" left="601">
					<g:Button text="Register" ui:field="btnRegsiter" styleName="{res.css.gwtButton}" />
				</g:at>
				<g:at top="470" left="319">
					<g:Label width="376px" height="70px" ui:field="lblError" styleName="{res.css.error}" />
				</g:at>
			</g:AbsolutePanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 