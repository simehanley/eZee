	<ui:with type="com.ezee.web.common.ui.css.EzeeDefaultResources" field="res" />

	<g:HTMLPanel>
		<g:AbsolutePanel width="540px" height="114px" styleName="{res.css.gwtPanelBorder}">
			<g:at top="10" left="10">
				<g:Label styleName="{res.css.gwtLabelSmall}" width="507px" height="47px" ui:field="lblMessage" wordWrap="true" />
			</g:at>
			<g:at top="72" left="435">
				<g:Button text="Close" ui:field="btnClose" styleName="{res.css.gwtButton}" />
			</g:at>
		</g:AbsolutePanel>
