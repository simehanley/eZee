			<g:at top="74" left="174">
				<g:TextBox styleName="{res.css.gwtTextBoxLarge}" ui:field="txtAddressLine2" />
			</g:at>
			<g:at top="106" left="174">
				<g:TextBox styleName="{res.css.gwtTextBoxLarge}" ui:field="txtSuburb" />
			</g:at>
			<g:at top="138" left="174">
				<g:TextBox styleName="{res.css.gwtTextBoxLarge}" ui:field="txtCity" />
			</g:at>
			<g:at top="170" left="174">
				<g:ListBox styleName="{res.css.gwtListBoxSmall}" ui:field="lstState" />
			</g:at>
			<g:at top="203" left="174">
				<g:TextBox styleName="{res.css.gwtTextBoxSmall}" ui:field="txtPostCode" />
			</g:at>
			<g:at top="235" left="174">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtPhone" />
			</g:at>
			<g:at top="267" left="174">
				<g:TextBox styleName="{res.css.gwtTextBoxMediumEmail}" ui:field="txtEmail" />
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 