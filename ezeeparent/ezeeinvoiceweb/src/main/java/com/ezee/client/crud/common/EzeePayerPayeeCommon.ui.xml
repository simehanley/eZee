<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="com.ezee.web.common.ui.css.EzeeDefaultResources" field="res" />

	<g:HTMLPanel>
		<g:AbsolutePanel height="370px" width="327px">
			<g:at top="10" left="10">
				<g:Label text="Name" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="42" left="10">
				<g:Label text="Contact" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="74" left="10">
				<g:Label text="Address 1" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="106" left="10">
				<g:Label text="Address 2" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="138" left="10">
				<g:Label text="Suburb" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="170" left="10">
				<g:Label text="City" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="202" left="10">
				<g:Label text="State" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="235" left="10">
				<g:Label text="Post Code" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="267" left="10">
				<g:Label text="Phone" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="299" left="10">
				<g:Label text="Fax" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="331" left="10">
				<g:Label text="EMail" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="10" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtName" />
			</g:at>
			<g:at top="42" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtContact" />
			</g:at>
			<g:at top="74" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtAddressLine1" />
			</g:at>
			<g:at top="106" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtAddressLine2" />
			</g:at>
			<g:at top="138" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtSuburb" />
			</g:at>
			<g:at top="170" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtCity" />
			</g:at>
			<g:at top="202" left="102">
				<g:ListBox styleName="{res.css.gwtListBoxSmall}" ui:field="lstState" />
			</g:at>
			<g:at top="235" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxSmall}" ui:field="txtPostCode" />
			</g:at>
			<g:at top="267" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtPhone" />
			</g:at>
			<g:at top="299" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtFax" />
			</g:at>
			<g:at top="331" left="102">
				<g:TextBox styleName="{res.css.gwtTextBoxMediumEmail}" ui:field="txtEmail" />
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 