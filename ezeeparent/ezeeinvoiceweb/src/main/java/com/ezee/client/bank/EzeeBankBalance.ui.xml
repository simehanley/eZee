<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<ui:with type="com.ezee.client.css.EzeeInvoiceDefaultResources" field="res" />

	<g:HTMLPanel>
		<g:AbsolutePanel height="464px" width="919px">
			<g:at top="10" left="10">
				<g:Label text="Premises" styleName="{res.css.gwtLabelMedium}" />
			</g:at>
			<g:at top="9" left="173">
				<g:ListBox styleName="{res.css.gwtListBoxMedium}" ui:field="lstPremises" />
			</g:at>
			<g:at top="44" left="10">
				<g:Label text="Outstanding Cheques" styleName="{res.css.gwtLabelMedium}" />
			</g:at>
			<g:at top="79" left="12">
				<g:ResizeLayoutPanel width="891px" height="303px" styleName="{res.css.gwtPanelBorder}">
					<p1:DataGrid width="100%" height="100%" ui:field="grdCheques" />
				</g:ResizeLayoutPanel>
			</g:at>
			<g:at top="388" left="12">
				<g:Label text="Balance" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="388" left="99">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" ui:field="txtBalance" />
			</g:at>
			<g:at top="388" left="311">
				<g:Label text="Cheques" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="388" left="398">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" enabled="false" />
			</g:at>
			<g:at top="388" left="610">
				<g:Label text="Net" styleName="{res.css.gwtLabelSmall}" />
			</g:at>
			<g:at top="388" left="697">
				<g:TextBox styleName="{res.css.gwtTextBoxMedium}" enabled="false" />
			</g:at>
			<g:at top="421" left="723">
				<g:Button ui:field="btnRefresh" text="Refresh" styleName="{res.css.gwtButton}" />
			</g:at>
			<g:at top="421" left="816">
				<g:Button ui:field="btnClose" text="Close" styleName="{res.css.gwtButton}" />
			</g:at>

		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 